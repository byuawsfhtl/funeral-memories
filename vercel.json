{
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"rewrites": [
		{
			"source": "/api/(.*)",
			"destination": "/api/$1.js"
		},
		{
			"source": "/(.*)",
			"destination": "/"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"methods": ["GET", "POST", "PUT", "DELETE"],
			"dest": "/api/$1",
			"headers": {
				"x-vercel-protection-bypass": "1"
			}
		}
	]
}
