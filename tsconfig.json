{
	"compilerOptions": {
		"target": "esnext",
		"jsx": "react-jsx",
		"module": "esnext",
		"moduleResolution": "node",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"allowJs": true,
		"noEmit": true // <--- ðŸ’¥ Prevents overwriting .js files!
	},
	"include": ["**/*.ts", "**/*.tsx"],
	"exclude": ["node_modules"]
}
